//Write a C# Sharp program to check which number is closest to 100 among two given integers. Return 0 if the two numbers are equal.

Console.WriteLine("Please enter a number");
int num1 = Convert.ToInt32(Console.ReadLine());
int num11 = num1;
Console.WriteLine("Please enter a second number");
int num2 = Convert.ToInt32(Console.ReadLine());
int num22 = num2;

if (num1 == num2)
{
    Console.WriteLine(0);
}
else 
{
    num11 -= 100;
    num22 -= 100;
    if (num11 >= num22)
    { Console.WriteLine(num1 + " is closer to 100");}
    else
    { Console.WriteLine(num2 + " is closer to 100");}
}

Console.ReadKey();
